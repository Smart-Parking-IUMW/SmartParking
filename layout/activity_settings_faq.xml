<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingsFAQ">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="194dp"
        android:layout_marginRight="194dp"
        android:layout_marginBottom="50dp"
        android:onClick="btn_homepage"
        app:layout_constraintEnd_toStartOf="@+id/viewprofile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <ImageView
        android:id="@+id/viewprofile"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginStart="194dp"
        android:layout_marginLeft="194dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="100dp"
        android:onClick="btn_profile"
        app:layout_constraintStart_toEndOf="@+id/logo"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profile" />

    <ImageView
        android:id="@+id/homepage"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginBottom="8dp"
        android:onClick="btn_homepage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/home" />

    <ImageView
        android:id="@+id/reservation"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="4dp"
        android:onClick="btn_reservation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/homepage"
        app:srcCompat="@drawable/reservation" />

    <ImageView
        android:id="@+id/scan"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="10dp"
        android:onClick="btn_scan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/profile"
        app:layout_constraintHorizontal_bias="0.038"
        app:layout_constraintStart_toEndOf="@+id/reservation"
        app:srcCompat="@drawable/scan" />

    <ImageView
        android:id="@+id/payment"
        android:layout_width="49dp"
        android:layout_height="52dp"
        android:onClick="btn_payment"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/scan"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/scan"
        app:srcCompat="@drawable/payment" />

    <ImageView
        android:id="@+id/profile"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="14dp"
        android:onClick="btn_menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/payment"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/payment"
        app:srcCompat="@drawable/menu" />

    <TextView
        android:id="@+id/faq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginBottom="415dp"
        android:text="      FAQ"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/homepage"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/faq1"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:gravity="start|center"
        android:text="   FAQ 1                                                                                  >"
        tools:layout_editor_absoluteX="16dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/faq"
        tools:layout_editor_absoluteY="152dp" />

    <Button
        android:id="@+id/faq2"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:gravity="start|center"
        android:text="   FAQ 2                                                                                  >"
        tools:layout_editor_absoluteX="16dp"
        app:layout_constraintTop_toBottomOf="@+id/faq1"
        tools:layout_editor_absoluteY="152dp" />

    <Button
        android:id="@+id/faq3"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:gravity="start|center"
        android:text="   FAQ 3                                                                                  >"
        tools:layout_editor_absoluteX="16dp"
        app:layout_constraintTop_toBottomOf="@+id/faq2"
        tools:layout_editor_absoluteY="152dp" />

    <Button
        android:id="@+id/faq4"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:gravity="start|center"
        app:layout_constraintTop_toBottomOf="@+id/faq3"
        android:text="   FAQ 4                                                                                  >"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="152dp" />

    <Button
        android:id="@+id/faq5"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:gravity="start|center"
        app:layout_constraintTop_toBottomOf="@+id/faq4"
        android:text="   FAQ 5                                                                                  >"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="152dp" />

    <Button
        android:id="@+id/faq6"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:gravity="start|center"
        app:layout_constraintTop_toBottomOf="@+id/faq5"
        android:text="   FAQ 6                                                                                  >"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="152dp" />


</androidx.constraintlayout.widget.ConstraintLayout>